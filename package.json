{
  "name": "pressly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env next build",
    "dev:local": "cross-env ENVIRONMENT=development next dev",
    "dev:prod": "cross-env ENVIRONMENT=production next dev",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src/ --ext .ts,.tsx --config .eslintrc.json --fix",
    "prepare": "husky install",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:changed": "jest -o",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component",
    "typeorm": "typeorm-ts-node-commonjs -d ./server/configs/db.ts",
    "migration:generate": "npm run typeorm -- migration:generate ./server/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run"
  },
